<%- include('./includes/header.ejs') %>
<title>Login</title>
<meta name="robots" content="noindex,nofollow">
<!--FOR SEO SHOULD HAVE THIS ROOT URL-->
<link rel="canonical" href="https://brisbaneflamenco.com.au/getLogin">
<!-- <meta name="viewport" content="width=device-width, initial-scale=1"> -->
<link rel="stylesheet" href="/css/getLogin.css">
<%- include('./includes/nav.ejs') %>
<div id="mainContainer">
  <h1>Login</h1>

  <form action="/admin/loginto-flamenco-admin" method="POST">
    <input id="loginEmail" name="loginEmail" type="email" placeholder="Email">
    <input id="loginPassword" name="loginPassword" type="password" placeholder="Password">
    <button id="btnLogin">Log in</button>
    
  </form>
  <a href="/forgot-password">Forgot Password?</a>
  <h2>Sign up if you are new</h2>
  <!-- createNewUser form -->
  <form action="/create-new-user" method="get">
    <button id="btnSignup">Sign up</button>
  </form>

</div>
